{
	"name": "@isopodlabs/maths",
	"description": "Maths utilties",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adrianstephens/ts-maths.git"
	},
	"exports": {
		"./vector": {
			"require": "./dist/vector.js",
			"import": "./esm/vector.js",
			"browser": "./esm/vector.js"
		},
		"./rational": {
			"require": "./dist/rational.js",
			"import": "./esm/rational.js",
			"browser": "./esm/rational.js"
		},
		"./complex": {
			"require": "./dist/complex.js",
			"import": "./esm/complex.js",
			"browser": "./esm/complex.js"
		},
		"./quaternion": {
			"require": "./dist/quaternion.js",
			"import": "./esm/quaternion.js",
			"browser": "./esm/quaternion.js"
		},
		"./geometry": {
			"require": "./dist/geometry.js",
			"import": "./esm/geometry.js",
			"browser": "./esm/geometry.js"
		},
		"./polynomial": {
			"require": "./dist/polynomial.js",
			"import": "./esm/polynomial.js",
			"browser": "./esm/polynomial.js"
		}
	},
	"files": [
		"dist/**/*.{js,d.ts}",
		"esm/**/*.{js,d.ts}"
	],
	"scripts": {
		"build": "tsc -p ./tsconfig.json & tsc -p ./tsconfig.esm.json",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"test": "node -r ts-node/register tests/run-tests.ts"
	},
	"keywords": [
		"math-utils",
		"linear-algebra",
		"matrices",
		"vectors",
		"quaternion",
		"rotation",
		"euler-angles",
		"axis-angle",
		"characteristic-polynomial",
		"eigenvalues",
		"matrix-inverse",
		"matrix-ops",
		"typescript",
		"unit-quaternion",
		"complex",
		"rational",
		"polynomial",
		"geometry",
		"generics",
		"graphics",
		"robotics"
	],
	"author": "Adrian Stephens",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.13.8",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"eslint": "^9.21.0",
		"ts-node": "^10.9.2",
		"ts-patch": "^3.3.0",
		"typescript-eslint": "^8.26.0"
	},
	"version": "0.5.0"
}
