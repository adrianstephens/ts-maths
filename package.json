{
	"name": "@isopodlabs/maths",
	"description": "Maths utilties",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adrianstephens/ts-maths.git"
	},
	"exports": {
		"./core": {
			"types": "./dist/core.d.ts",
			"require": "./dist/core.js",
			"import": "./esm/core.js"
		},
		"./integer": {
			"types": "./dist/integer.d.ts",
			"require": "./dist/integer.js",
			"import": "./esm/integer.js"
		},
		"./real": {
			"types": "./dist/real.d.ts",
			"require": "./dist/real.js",
			"import": "./esm/real.js"
		},
		"./rational": {
			"types": "./dist/rational.d.ts",
			"require": "./dist/rational.js",
			"import": "./esm/rational.js"
		},
		"./complex": {
			"types": "./dist/complex.d.ts",
			"require": "./dist/complex.js",
			"import": "./esm/complex.js"
		},
		"./quaternion": {
			"types": "./dist/quaternion.d.ts",
			"require": "./dist/quaternion.js",
			"import": "./esm/quaternion.js"
		},
		"./vector": {
			"types": "./dist/vector.d.ts",
			"require": "./dist/vector.js",
			"import": "./esm/vector.js"
		},
		"./geometry": {
			"types": "./dist/geometry.d.ts",
			"require": "./dist/geometry.js",
			"import": "./esm/geometry.js"
		},
		"./polynomial": {
			"types": "./dist/polynomial.d.ts",
			"require": "./dist/polynomial.js",
			"import": "./esm/polynomial.js"
		},
		"./linear": {
			"types": "./dist/linear.d.ts",
			"require": "./dist/linear.js",
			"import": "./esm/linear.js"
		},
		"./mvpolynomial": {
			"types": "./dist/mvpolynomial.d.ts",
			"require": "./dist/mvpolynomial.js",
			"import": "./esm/mvpolynomial.js"
		},
		"./gen": {
			"types": "./dist/gen.d.ts",
			"require": "./dist/gen.js",
			"import": "./esm/gen.js"
		},
		"./string": {
			"types": "./dist/string.d.ts",
			"require": "./dist/string.js",
			"import": "./esm/string.js"
		},
		"./big": {
			"types": "./dist/big.d.ts",
			"require": "./dist/big.js",
			"import": "./esm/big.js"
		},
		"./prime": {
			"types": "./dist/prime.d.ts",
			"require": "./dist/prime.js",
			"import": "./esm/prime.js"
		},
		"./statistics": {
			"types": "./dist/statistics.d.ts",
			"require": "./dist/statistics.js",
			"import": "./esm/statistics.js"
		}
	},
	"files": [
		"dist/**/*.{js,d.ts}",
		"esm/**/*.{js,d.ts}"
	],
	"scripts": {
		"build": "tsc -p ./tsconfig.json & tsc -p ./tsconfig.esm.json",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"test": "node -r ts-node/register tests/run-tests.ts"
	},
	"keywords": [
		"math-utils",
		"linear-algebra",
		"matrices",
		"vectors",
		"quaternion",
		"rotation",
		"euler-angles",
		"axis-angle",
		"characteristic-polynomial",
		"eigenvalues",
		"matrix-inverse",
		"matrix-ops",
		"typescript",
		"unit-quaternion",
		"complex",
		"rational",
		"polynomial",
		"geometry",
		"generics",
		"graphics",
		"robotics"
	],
	"author": "Adrian Stephens",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.13.8",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"eslint": "^9.21.0",
		"ts-node": "^10.9.2",
		"ts-patch": "^3.3.0",
		"typescript-eslint": "^8.26.0"
	},
	"version": "1.0.0",
	"dependencies": {
		"source-map": "^0.7.6"
	}
}
